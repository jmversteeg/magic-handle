{
  "name":            "worq",
  "version":         "0.1.0",
  "description":     "Promise-based job queue that opens on demand and closes after a specified interval of inactivity",
  "main":            "lib/Worqer.js",
  "scripts":         {
    "docs":    "./node_modules/.bin/jsdoc ./lib/*.js -d ./docs",
    "pretest": "jshint lib",
    "test":    "mocha"
  },
  "engines":         {
    "node": ">=0.10.0"
  },
  "repository":      {
    "type": "git",
    "url":  "https://github.com/jmversteeg/worq"
  },
  "keywords":        [
    "worker",
    "promise",
    "promises",
    "q",
    "flow control",
    "handler",
    "threaded",
    "thread",
    "threads",
    "concurrency",
    "job",
    "tasks",
    "task",
    "queue",
    "util"
  ],
  "author":          "JM Versteeg",
  "license":         "MIT",
  "bugs":            {
    "url": "https://github.com/jmversteeg/worq/issues"
  },
  "homepage":        "https://github.com/jmversteeg/worq",
  "dependencies":    {
    "functionify": "^1.0.0",
    "lodash":      "^3.3.1",
    "q":           "^1.2.0"
  },
  "devDependencies": {
    "chai":             "^2.2.0",
    "chai-as-promised": "^4.3.0",
    "coveralls":        "^2.11.2",
    "jsdoc":            "^3.3.0-beta2",
    "jshint":           "^2.6.3",
    "mocha":            "^2.1.0",
    "sinon":            "^1.14.1",
    "sinon-chai":       "^2.7.0"
  }
}
